<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.pavel.ChatController">
   <children>
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="508.0">
         <children>
            <Label fx:id="chatTitle" alignment="CENTER" prefHeight="17.0" prefWidth="510.0" style="-fx-text-alignment: center;" text="Chat with Marry" />
            <TextArea fx:id="text" editable="false" prefHeight="446.0" prefWidth="508.0" text="" />
            <HBox prefHeight="100.0" prefWidth="583.0">
               <children>
                  <TextField fx:id="message" prefHeight="25.0" prefWidth="406.0" text="Hello" />
                  <Button fx:id="sendButton" mnemonicParsing="false" onAction="#sendButtonHandler" prefHeight="25.0" prefWidth="104.0" text="Send" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox prefHeight="400.0" prefWidth="136.0">
         <children>
            <Label alignment="CENTER" prefHeight="3.0" prefWidth="136.0" text="Rooms" />
            <ListView fx:id="personList" prefHeight="386.0" prefWidth="136.0" />
         </children>
      </VBox>
   </children>
</HBox>
